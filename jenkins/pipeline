    stages {
        stage("echo") {
            steps {
                echo "================start now================"
                sh 'pwd'
            }
        }
#        stage("clear") {
#            steps {
#                sh 'rm -rf $(pwd)/'
#            }
#        }
        stage("git clone") {
            steps {
                sh "git clone git@github.com:Shebs1/test-ci-cd.git" 
            }
        }
    }
}
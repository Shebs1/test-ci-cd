#!groovy


pipeline{
    agent{
        label 'master'
    }
    options {
        timestamps()
    }
    stages {
        stage("echo") {
            steps {
                echo "================start now================"
                sh 'pwd'
            }
        }
        stage("clear") {
            steps {
                sh 'rm -rf test_cicd'
            }
        }
    }
}